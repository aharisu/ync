{
  "name": "@aharisu/ync",
  "version": "0.0.2",
  "description": "Ync is a validator and parser inspired by lizod, which in turn was inspired by zod.",
  "keywords": [
    "typescript",
    "schema",
    "validation",
    "type",
    "zod"
  ],
  "bugs": {
    "url": "https://github.com/aharisu/ync/issues"
  },
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aharisu/ync.git"
  },
  "author": "aharisu",
  "license": "MIT",
  "scripts": {
    "test": "vitest",
    "typecheck": "vitest typecheck",
    "coverage": "vitest run --coverage",
    "build": "vite build && tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  }
}
